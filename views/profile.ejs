<!DOCTYPE html>
<html>

<head>
    <title>Profil page</title>
    <link rel="icon" href="">
    <link rel="stylesheet" href="css/profile.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

</head>

<body>
    <div>
        <input type="text" placeholder="Search" id="search_id">
    </div>
    <div class="profile">

        <div class="left">
            <img id="profile_picture_id" src="" alt="user">

            <div class="profil-photo">
                <form id="form_upload_pic_id" action="">
                <label for="photo"><i class="fa fa-camera" style="color:#0247fe; font-size: 15px;"></i></label>
                <input type="file" id="photo">
            </form>
            </div>

            <h2 id="first_last_name_id"></h2>
        </div>

        <div class="right">
            <button id="div_add_friend_button" type="button" class="friend"><i class="fa fa-user-plus" style="font-size:20px"></i>Add
                Friend</button>
            <button type="button" class="message"><i class="fa fa-envelope" style="font-size:20px"></i> Message</button>
        </div>
    </div>

    <div class="conteiner">

        <div class="about">
            <h4>About</h4>
            <ul>
                <li><i class="fa fa-home"></i> From Doboj,BIH</li>
                <li><i class="fa fa-map-marker"></i> Lives in Doboj,BIH</li>
                <li><i class="fa fa-birthday-cake"></i> Birthday 7.12.1996.</li>
                <li><i class="fa fa-male"></i><i class="fa fa-female"></i> Gender male</li>
            </ul>
        </div>

        <div class="write-post">

            <h4>Post</h4>

            <div class="post">
                <img id="postUserImage" src="" alt="user" class="user">
                <textarea id="sometext" placeholder="Write post" cols="65" rows="4"></textarea>
            </div>

            <div class="add-photo">
                <form id="form_post_id"  enctype="multipart/form-data" method="post">
                <label for="add"><i class="fa fa-image" style="font-size:24px; color:#80B25D;"></i>
                    <small style="color:rgb(80, 75, 75);">Add image</small></label>
                <input type="file" name="profile_pic" id="add">
                <button id="btn_upload_post_id" ><strong>Post</strong> </button>
            </form>
            </div>
        </div>

        <div class="slike">
            <div class="sve-slike">
                <h4>Fotografije</h4>
                <a href="">Sve fotografije</a>
            </div>
        </div>
<!-- post krece odavde -->
<div id="allMyPosts"></div>
        <div class="post2">
            <div class="user-profil">
                <img  src="" alt="user" class="user">
                <span class="span">
                    <h4 id= "first_name_id" style="color: rgb(51, 49, 49);">Zoran Blagojevic</h4> &nbsp; <p style="color: #726d6d;;">je
                        objavio post</p> &nbsp;
                </span>
                <small><i class="fa fa-clock-o"></i>&nbsp;22:42</small>
            </div>

            <div class="foto">
                <img  src="" width="550px" height="420px">
            </div>

            <div class="like-com">
                <button class="like"><i class="fa fa-thumbs-o-up" style="font-size:20px"></i> &nbsp; <p>Like 23</p>
                    </button>
                <button class="coment"><i class="fa fa-comments-o" style="font-size:20px"></i> &nbsp; <p>Comment 3</p>
                    </button>
            </div>

            <div class="write-com">
                <img src="" alt="user" class="user">
                <textarea placeholder="Napisi komentar..." cols="70" rows="3" class="text-com"></textarea>
            </div>
        </div>
